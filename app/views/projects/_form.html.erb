<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@project, url: yield(:url) ) do |f| %>
      <%= render 'shared/error_messages' , object: f.object %>

      <%= f.label :project_number %>
      <%= f.text_field :project_number ,class:'form-control' %>

      <%= f.label :name %>
      <%= f.text_field :name ,class:'form-control' %>

      <%= f.submit yield(:botton_text) , class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
